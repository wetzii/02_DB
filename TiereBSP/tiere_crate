CREATE TABLE Lebensraum (
    LID INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    Gebiet VARCHAR(50),
    Kontinent VARCHAR(50)
);
CREATE TABLE Tier (
    TID INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    Name VARCHAR(50),
    Art VARCHAR(50),
    GebYear INTEGER,
    Gewicht INTEGER,
    Futterart VARCHAR(50),
    LID INTEGER REFERENCES Lebensraum(LID)
);
